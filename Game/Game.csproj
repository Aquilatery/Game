  <PropertyGroup>
    <ApplicationIcon>Ico.ico</ApplicationIcon>
  </PropertyGroup>
  <PropertyGroup>
    <StartupObject>Game.Game</StartupObject>
  </PropertyGroup>
  <ItemGroup>
    <Content Include="Ico.ico" />
  </ItemGroup>
</Project>